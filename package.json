    "element-plus": "^2.4.2",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "fullpage.js": "^4.0.20",
    "gsap": "^3.12.5",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "pinia": "^2.0.21",
    "three": "^0.163.0",
    "vanilla-tilt": "^1.8.1",
    "vanta": "^0.5.24",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
